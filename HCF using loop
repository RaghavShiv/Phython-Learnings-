T=int(input())
    for i in range(T):
            X=input().split()
            A=int(X[0])
            B=int(X[1])
            #print(A,B)
            c=1
            for j in range(1, min(A,B)+1):
                if (( A % j == 0 )and ( B % j == 0 )):
                    C=j
                    
            print(C)
       
